{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.py"
    },
    {
      "src": "/?(.*)",
      "dest": "public/index.html"
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/public/index.html"
    }
  ]
}
